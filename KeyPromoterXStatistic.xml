<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Rerun 'Test retokeization_is_equivalent_to_tokenization'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'Test retokeization_is_equivalent_to_tokenization'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Test retokeization_is_equivalent_to_tokenization'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'Test retokeization_is_equivalent_to_tokenization'</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Test'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'Test'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>