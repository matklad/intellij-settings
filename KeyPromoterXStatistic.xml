<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Rerun 'Test retokeization_is_equivalent_to_tokenization'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'Test retokeization_is_equivalent_to_tokenization'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run to Cursor">
        <Item>
          <IdeaActionID>RunToCursor</IdeaActionID>
          <ShortCut>'Alt+F9'</ShortCut>
          <Description>Run to Cursor</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'RsStatementUpDownMoverTest'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'RsStatementUpDownMoverTest'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'RsStatementUpDownMoverTest.test step statement'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'RsStatementUpDownMoverTest.test step statement'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Test retokeization_is_equivalent_to_tokenization'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'Test retokeization_is_equivalent_to_tokenization'</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Test'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'Test'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>